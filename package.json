{
  "name": "@withuno/locust",
  "version": "0.1.0",
  "description": "A fork of @buttercup/locust with TypeScript support and new features incoming...",
  "author": "Uno <support@uno.app> (https://uno.app)",
  "contributors": [
    "Perry Mitchell <perry@perrymitchell>"
  ],
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.js"
  },
  "jsdelivr": "./dist/iife/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run clean:dist && melodist dev --outdir dist --platform browser -f cjs -f esm -f iife --name Locust",
    "build": "npm run clean:dist && melodist build --outdir dist --platform browser -f cjs -f esm -f iife --name Locust",
    "build:test": "npm run clean:test && melodist build --no-minify --outdir test/dist --platform browser -f iife --no-typecheck --name Locust",
    "clean": "run-s clean:*",
    "clean:dist": "rimraf dist",
    "clean:test": "rimraf test/dist",
    "serve": "serve ./dist/ -p 8081",
    "test": "run-s test:unit test:integration",
    "test:unit": "karma start --single-run",
    "test:unit:watch": "karma start --auto-watch",
    "test:integration": "npm run build:test && ts-node test/integration/index.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@ikscodes/eslint-config": "~8.2.0",
    "@ikscodes/prettier-config": "~2.2.0",
    "@types/chai": "~4.3.4",
    "@types/mocha": "~10.0.1",
    "@types/sinon": "~10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "~8.36.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-unused-imports": "~2.0.0",
    "eventemitter3": "^4.0.7",
    "husky": "8.0.3",
    "is-visible": "^2.2.0",
    "karma": "^6.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-sinon": "^1.0.5",
    "karma-spec-reporter": "^0.0.34",
    "karma-typescript": "~5.5.3",
    "lint-staged": "^13.2.0",
    "melodist": "~0.7.9",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.3",
    "prettier": "~2.8.4",
    "puppeteer": "~19.7.5",
    "puppeteer-extra": "~3.3.6",
    "puppeteer-extra-plugin-stealth": "~2.11.2",
    "rimraf": "^3.0.2",
    "serve": "^14.0.1",
    "sinon": "^14.0.0",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.5"
  }
}
